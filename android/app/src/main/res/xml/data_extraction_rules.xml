<?xml version="1.0" encoding="utf-8"?>
<!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/guide/topics/data/autobackup#XMLSyntax
   for the complete list of options.

   VettID: Backup disabled for security - credentials must never leave device
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude all files from cloud backup -->
        <exclude domain="root" />
        <exclude domain="file" />
        <exclude domain="database" />
        <exclude domain="sharedpref" />
        <exclude domain="external" />
    </cloud-backup>
    <device-transfer>
        <!-- Exclude all files from device-to-device transfer -->
        <exclude domain="root" />
        <exclude domain="file" />
        <exclude domain="database" />
        <exclude domain="sharedpref" />
        <exclude domain="external" />
    </device-transfer>
</data-extraction-rules>
