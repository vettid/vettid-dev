{
  "name": "vettid-scaffold",
  "version": "0.2.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -w",
    "cdk": "cdk",
    "bootstrap": "./node_modules/.bin/cdk bootstrap",
    "deploy": "npm run build && ./node_modules/.bin/cdk deploy --all",
    "synth": "npm run build && ./node_modules/.bin/cdk synth",
    "destroy": "cdk destroy --all",
    "ensure-default-terms": "./node_modules/.bin/cdk deploy --outputs-file cdk-outputs.json && aws lambda invoke --function-name $(cat cdk-outputs.json | grep EnsureDefaultTermsLambdaArn | awk '{print $2}' | tr -d '\",' | awk -F: '{print $NF}') /tmp/ensure-terms-response.json && cat /tmp/ensure-terms-response.json",
    "test": "jest --config tests/jest.config.js",
    "test:unit": "jest --config tests/jest.config.js --testPathPattern=unit",
    "test:integration": "jest --config tests/jest.config.js --testPathPattern=integration",
    "test:e2e": "jest --config tests/jest.config.js --testPathPattern=e2e",
    "test:security": "jest --config tests/jest.config.js --testPathPattern=security",
    "test:coverage": "jest --config tests/jest.config.js --coverage"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.939.0",
    "@aws-sdk/s3-request-presigner": "^3.939.0",
    "@types/aws-lambda": "^8.10.159",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.10.1",
    "@types/pdfkit": "^0.17.3",
    "aws-cdk": "2.1033.0",
    "esbuild": "0.27.0",
    "jest": "^29.7.0",
    "nats": "^2.29.3",
    "pdfkit": "^0.17.2",
    "ts-jest": "^29.4.6",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@aws-cdk/cloud-assembly-schema": "48.20.0",
    "@aws-sdk/client-cloudwatch": "^3.939.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.940.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.939.0",
    "@aws-sdk/client-dynamodb": "^3.939.0",
    "@aws-sdk/client-ec2": "^3.946.0",
    "@aws-sdk/client-secrets-manager": "^3.947.0",
    "@aws-sdk/client-ses": "^3.939.0",
    "@aws-sdk/util-dynamodb": "^3.939.0",
    "@types/pg": "^8.15.6",
    "argon2": "^0.44.0",
    "aws-cdk-lib": "2.229.1",
    "constructs": "10.4.3",
    "nkeys.js": "^1.1.0",
    "pg": "^8.16.3",
    "source-map-support": "^0.5.21",
    "ulid": "^3.0.1"
  }
}
